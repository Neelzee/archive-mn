<script>
    //mediaqueries som legger til tekst over tabellen dersom man er på tlf.. og litt mer
    window.onload = function(){
        h4 = document.getElementById("sokResult").getElementsByTagName("h4")
        h4[0].style.display = "inline-block";
        var mq = window.matchMedia("(max-width: 420px)");
        lagt_til_mobil = false;
        lagt_til_pc = false;
        excelEle = document.createElement('div')
        excelEle.style.display = "inline-block"
        
            excelEle.innerHTML = '<a class="bold-text d-none d-md-block" style="font-size: 1.1rem;  display:inline; float:right; margin-right:2px;" href="/statistikk/excel_download/boker/385" onClick="return submitSearchForm(`/statistikk/excel_download/boker/385`);"> <img src="/images/excel_new.png" width="25" height="18" style="margin-right: 2px; margin-bottom:7px;" />Excel</a>'
        
        function mediaFunction(mediaqr){
            if(mediaqr.matches && !lagt_til_mobil){
                document.getElementById('forklaringTxt').style.marginBottom = '0';
                    if(document.getElementById("sokResult").classList=="col-12 scroll px-0 mt-2"){
                        newEle = document.createElement('h5')
                        newEle.style.color = "gray";
                        h4[0].insertBefore(newEle, h4[0].nextNode) 
                        lagt_til_mobil = true;
                        excelEleMobile = document.createElement('div')
                        
                            excelEleMobile.innerHTML = '<a class="bold-text" style="font-size: 1.1rem; float:right; position:absolute; right:0; " href="/statistikk/excel_download/boker/385" onClick="return submitSearchForm(`/statistikk/excel_download/boker/385`);"> <img src="/images/excel_new.png" width="20" height="15" style="margin-right: 2px;margin-bottom:7px;" />Excel</a>'
                        
                        newEle.innerHTML = "Sveip for å se hele tabellen <bold style='font-size:18px;'>&#8594;</bold>"
                        newEle.innerHTML += excelEleMobile.innerHTML
                        newEle.classList.add("d-md-none")
                        document.getElementById("sokResult").classList = "col-12 scroll px-0 mt-4" 
                    }else if(document.getElementById("sokResult").classList =="col-12 px-0 mt-2"){
                        document.getElementById("sokResult").classList = "col-12 px-0 mt-4" 
                    }        
            }else{
                if(!lagt_til_pc){
                    document.getElementById("sokResult").insertAdjacentHTML('afterbegin', excelEle.innerHTML);
                    lagt_til_pc = true;
                }
                
            }
        }
        mediaFunction(mq)
        mq.addListener(mediaFunction)
    }
</script> 
