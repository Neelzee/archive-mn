	<script>
	window.addEventListener("load", function() {
		/**
		 * Hindre at dialogboksen vises og for utvalgte IP'er.
		 */
		
			return;
		
		window.cookieconsent.initialise({
			window: '<div role="dialog" aria-label="cookieconsent" aria-describedby="cookieconsent:desc" class="cc-window {{classes}}" id="consentbanner" style="padding:20px;">{{children}}</div>',
			position: "bottom-left",
			content: {
				header: 'Informasjon',
				message: "Vi bruker informasjonskapsler (\"cookies\") på nettsiden vår til støtte for brukeropplevelsen og for å samle inn brukerstatistikk. Vi lagrer ikke opplysninger som kan spores tilbake til deg som person.",
				deny: "Tillat ikke",
				allow: "Tillat",
				dismiss: "Lukk",
				link: "Les vår personvernerklæring her",
				href: "https://medienorge.uib.no/personvern",
				
			},
			elements: {
				messagelink: '<span id="cookieconsent:desc" class="cc-message" style="margin-bottom:5px;">{{message}}<a aria-label="learn more about cookies" tabindex="0" class="cc-link" href="{{href}}" target="_blank" style="color:#DCF968; font-weight:600; text-decoration:none;">{{link}}</a></span>',
				dismiss: '<a aria-label="dismiss cookie message" role="button" tabindex="0" class="cc-btn cc-dismiss" style="top:0px; font-size: 1em;">{{dismiss}}</a>'
			},
			palette: {
				popup: {
				  	background: "rgba(74,171,203,0.8)",
				  	text: "white"
				},
				button: {
					background: "#BCD948"
				}
			},
			/**
			 * onPopupOpen eksekveres hver gang dialogvinduet vises, dvs. når brukeren ikke har har trykket "Lukk"/dismiss.
			 */
			onPopupOpen: function() {
				analytics();
				updateLog();
			},
			/**
			 * onInitialize eksekveres når det finnes en cookie, dvs. når brukeren har trykket "Lukk".
			 */
			onInitialise: function(status) {
				if(this.hasAnswered() && status != 'deny') {
					analytics();
					updateLog();
				}
			}
		});
	});
	</script>

