	<script>
	$(document).ready(function() {
		

		if($("table").width() > 975) {

			$("th").css("padding-right","5px").css("padding-left","5px");
			$("tr td:last-child").remove();
			$("tr th:last-child").remove();
		}

		// Hack som legger til tom rad under 2018
		$("tr").filter(function() {
			return $(this).children(":first").text() == '2018';
		}).nextAll().filter(function() {
			return $(this).children(":first").text().match(/[0-9+]/);
		}).first().before(function() {
			let row = '<tr class="one final" style="height:24px">'
			for(let x = 0; x < this.children.length; x++) {
				row += '<td></td>';
			}
			row += '</tr>';
			return row;
		});

		$("table").show();

	});
	</script>

