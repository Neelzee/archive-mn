<script>
$.ajaxSetup({ cache: false });
function load_mediehus(changes) {
			var hus = 'NRK,Schibsted,Egmont,TV 2,VG,Amedia,MTG,Discovery Networks,Dagbladet,Aftenposten,Edda Media,Bauer Media,NHST Media,Polaris Media,Dagens Næringsliv,Mentor Medier';
			if(changes) {
				if($("#mediehus :selected").length>0) {
					hus = $("#mediehus").val().join();
				} else {
					hus = "";
				}
			}

			$.post("/ajax/sok/366.cfm",{hus:hus,aar:$("#aar").val().join() }, function(data) {
				$("#mediehus").html(data);
			})
			.fail(function (data) {
				//alert("En teknisk feil oppstod ved utvalgsprosessen."+JSON.stringify(data));
				console.log("Feil under mottak av data:");
				console.log(JSON.stringify(data));
			});
}

$(document).ready(function (){

	$("#aar").change(function() {
	load_mediehus(true);

	});

	load_mediehus(false);


	$("#frmSearch").submit(function (e) {
		if($("#mediehus").val().length == 0) {
			$( function() {
				$( "#dialog-message" ).dialog({
				modal: true,
				buttons: {
					Ok: function() {
					$( this ).dialog( "close" );
					}
				}
				});
			} );
			e.preventDefault();
		}
	});
});
</script>

<div id="dialog-message" style="display: none" title="Informasjon mangler">
	<p>
	  Det finnes ikke informasjon for mediehuset du har valgt for dette året.
	</p>
  </div>

<form id="frmSearch" class="search" action="" method="post" enctype="application/x-www-form-urlencoded" style="width: 200px;">
	<fieldset>

		<div class="divLabel">velg mediehus/konsern:</div>
        <select name="mediehus" multiple="multiple" id="mediehus"></select>

